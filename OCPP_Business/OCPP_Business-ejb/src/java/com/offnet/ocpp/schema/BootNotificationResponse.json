{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "BootNotificationResponse",
  "type": "object",
  "properties": {
    "status": {
      "type": "string",
      "enum": [
        "Accepted",
        "Rejected"
      ]
    },
    "currentTime": {
      "type": "string",
      "format": "date-time"
    },
    "heartbeatInterval": {
      "type": "number"
    }
  },
  "required": [
    "status",
    "currentTime",
    "heartbeatInterval"
  ]
}